{
    "model_name": "WaveRNN",
    "model_description": "Mulaw and 9 bits",

    "audio":{
        "audio_processor": "audio",        
        "num_mels": 80,         
        "num_freq": 2049,       
        "sample_rate": 48000,   
        "frame_length_ms": 50,  
        "frame_shift_ms": 12.5, 
        "preemphasis": 0.98,    
        "min_level_db": -100,   
        "ref_level_db": 20,     
        "power": 1.5,           
        "griffin_lim_iters": 60,
        
        "signal_norm": true,    
        "symmetric_norm": true, 
        "max_norm": 4,          
        "clip_norm": true,      
        "mel_fmin": 125.0,         
        "mel_fmax": 7600.0,        
        "do_trim_silence": true  
    },

    "distributed":{
        "backend": "nccl",
        "url": "tcp:\/\/localhost:54321"
    },

    "epochs": 10000,
    "grad_clip": 1000,
    "lr": 0.0001,
    "warmup_steps": 100,
    "batch_size": 50,
    "checkpoint_step": 1000,
    "print_step": 1,
    "num_workers": 4,
    "mel_len": 8,
    "pad": 2,
    "use_aux_net": true,
    "use_upsample_net": true,
    "upsample_factors": [5, 5, 24],
    "mode": 9,
    "mulaw": true,

    "data_path": "/search/hadoop07/wuzijun/voice_data/Biaobei/Mels/",
    "output_path": "./output"
}
